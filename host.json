{
  "version": "2.0",
  "functionTimeout": "00:10:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "cors": {
        "allowedOrigins": [ "*" ],
        "allowedMethods": [ "GET", "POST", "OPTIONS" ],
        "allowedHeaders": [ "*" ],
        "allowCredentials": true
      }
    }
  }
}